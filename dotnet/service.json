{
  "stack": "dotnet",
  "memory": 256,
  "runtimeArgs": [
  ],
  "routes": {
    "printHelloWorld": {
      "path": "/api/vtex/hello",
      "public": true
    },
    "printHello": {
      "path": "/api/vtex/hello/:name",
      "public": true
    },
    "printPrivateValue": {
      "path": "/api/vtex/private",
      "public": false
    },
    "apiDefault": {
      "path": "/api/*url",
      "public": true
    },
    "default": {
      "path": "/*url",
      "public": true
    }
  },
  "events": {
    "onAppsLinked": {
      "sender": "apps",
      "keys": ["linked"]
    }
  }
}
